<AbsoluteLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/AbsoluteLayout1"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/fondo"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context=".MainActivity" >

    <ImageView
        android:id="@+id/ivContacto"
        android:layout_width="325dp"
        android:layout_height="298dp"
        android:layout_x="23dp"
        android:layout_y="107dp"
        android:src="@drawable/datos" />

    <EditText
        android:id="@+id/etPass"
        android:layout_width="267dp"
        android:layout_height="wrap_content"
        android:layout_x="51dp"
        android:layout_y="367dp"
        android:ems="10"
        android:inputType="textPassword" />

    <EditText
        android:id="@+id/etUsuario"
        android:layout_width="270dp"
        android:layout_height="wrap_content"
        android:layout_x="51dp"
        android:layout_y="254dp"
        android:ems="10" >

        <requestFocus />
    </EditText>

    <ImageView
        android:id="@+id/ivLogo1"
        android:layout_width="243dp"
        android:layout_height="82dp"
        android:layout_x="60dp"
        android:layout_y="13dp"
        android:src="@drawable/logo" />

    <ImageButton
        android:id="@+id/imageButton1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_x="66dp"
        android:layout_y="413dp"
        android:onClick="Conectar"
        android:src="@drawable/btnentrar" />

</AbsoluteLayout>
