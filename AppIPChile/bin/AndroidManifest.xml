<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="example.appipchile"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="9"
        android:targetSdkVersion="18" />
      <uses-permission android:name="android.permission.INTERNET"/>

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="example.appipchile.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="example.appipchile.Menu"></activity>
        <activity android:name="example.appipchile.Sede"></activity>
        <activity android:name="example.appipchile.video"></activity>
        <activity android:name="example.appipchile.Evaluacion"></activity>
        <activity android:name="example.appipchile.Horario"></activity>
        <activity android:name="example.appipchile.Contacto"></activity>
    </application>
    
 

</manifest>
